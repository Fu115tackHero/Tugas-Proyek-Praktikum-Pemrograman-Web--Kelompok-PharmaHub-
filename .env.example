# Midtrans Configuration
# Set DEMO_MODE=true untuk simulasi pembayaran tanpa credentials Midtrans
# Set DEMO_MODE=false untuk menggunakan Midtrans real
DEMO_MODE=false

# Midtrans Credentials (Dapatkan dari dashboard.midtrans.com)
MIDTRANS_SERVER_KEY=your-server-key-here
MIDTRANS_CLIENT_KEY=your-client-key-here

# Production mode (true untuk production, false untuk sandbox)
MIDTRANS_IS_PRODUCTION=false
